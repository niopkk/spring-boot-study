<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bbz.dao.OrderMapper">
    <insert id="createOrder" useGeneratedKeys="true" parameterType="com.bbz.model.Order">
     insert into orders (status, ports, update_time)
     VALUES (#{status},#{ports},#{updateTime})
    </insert>
</mapper>
