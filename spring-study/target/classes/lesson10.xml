<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="javaCollection" class="com.xml_study.lesson10.JavaCollection">
        <property name="list">
            <list>
                <value>张三</value>
                <value>张三1</value>
                <value>张三2</value>
                <value>张三3</value>
                <value>张4</value>
                <value>张三5</value>
            </list>
        </property>
        <property name="map">
            <map>
                <entry key="1" value="张三1"/>
                <entry key="2" value="张三2"/>
                <entry key="3" value="张三3"/>
                <entry key="4" value="张三4"/>
                <entry key="5" value="张三5"/>
            </map>
        </property>
        <property name="set">
            <set>
                <value>张三1</value>
                <value>张三2</value>
                <value>张三3</value>
                <value>张三4</value>
                <value>张三5</value>
            </set>
        </property>
        <property name="properties">
            <props>
                <prop key="1">张三1</prop>
                <prop key="2">张三2</prop>
                <prop key="3">张三3</prop>
                <prop key="4">张三4</prop>
                <prop key="5">张三5</prop>
            </props>
        </property>
    </bean>

</beans>